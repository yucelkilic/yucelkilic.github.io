@article{french1982,
title = {The upper atmosphere of Uranus: A critical test of isotropic turbulence models},
journal = {Icarus},
volume = {51},
number = {3},
pages = {491-508},
year = {1982},
issn = {0019-1035},
doi = {https://doi.org/10.1016/0019-1035(82)90142-7},
url = {https://www.sciencedirect.com/science/article/pii/0019103582901427},
author = {Richard G. French and James L. Elliot and Bruno Sicardy and Philip Nicholson and Keith Matthews},
abstract = {Observations of the 15 August 1980 Uranus occultation of KM 12, obtained from Cerro Tololo InterAmerican Observatory, European Southern Observatory, and Cerro Las Campanas Observatory, are used to compare the atmospheric structure at points separated by ∼140 km along the planetary limb. The results reveal striking, but by no means perfect, correlation of the light curves, ruling out isotropic turbulence as the cause of the light curve spikes. The atmosphere is strongly layered, and any acceptable turbulence model must accommodate the axial ratios of ⪆60 which are observed. The mean temperature of the atmosphere is 150 ± 15°K for the region near number density 1014 cm−3. Derived temperature variations of vertical scale ∼ 130km and amplitude ±5°K are in agreement for all stations, and correlated spikes correspond to low-amplitude temperature variations with a vertical scale of several kilometers.}
}

